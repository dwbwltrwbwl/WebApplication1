@model IEnumerable<WebApplication1.Models.Role>

@{
    ViewData["Title"] = "Index";
}

<h1 style="color: #94a728;">Роли</h1>

<table class="table">
    <thead></thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.role)
            </td>
        </tr>
}
    </tbody>
</table>
